{
  "version": 2,
  "builds": [
    { "src": "api/index.ts", "use": "@vercel/node" },
    { "src": "src/frontend/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/uploads.*", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "src/frontend/index.html" }
  ]
}
